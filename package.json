    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-backend": "^2.2.0",
    "isbot": "^3.6.7",
    "keen-slider": "^6.8.5",
    "lucide-react": "^0.125.0",
    "nprogress": "^0.2.0",
    "pretty-cache-header": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-wrap-balancer": "^0.4.0",
    "remix-i18next": "^5.1.0",
    "remix-utils": "^6.0.0",
