{
  "name": "web3-frontend-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run codegen && remix build",
    "dev": "npm-run-all --parallel dev:*",
    "dev:remix": "remix dev",
    "dev:codegen": "npm run codegen -- --watch",
    "start": "remix-serve build",
    "codegen": "node -r dotenv/config node_modules/.bin/gql-gen",
    "lint": "prettier --write 'app/**/{*.ts,*.tsx}' && eslint --fix --no-error-on-unmatched-pattern 'app/**/{*.ts,*.tsx}'",
    "prepare": "husky install",
    "deploy": "fly deploy --remote-only",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.0.16",
    "@m5r/og": "^0.1.2",
    "@radix-ui/react-accordion": "^1.1.2",
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-run/serve": "^1.19.3",
    "@treasure-project/tailwind-config": "^1.0.0",
    "clsx": "^1.2.1",
    "dotenv": "^16.0.3",
    "framer-motion": "^10.9.1",
    "graphql": "^16.6.0",
    "graphql-request": "5.1.0",
    "graphql-tag": "^2.12.6",
    "i18next": "^22.4.13",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-backend": "^2.2.0",
    "isbot": "^3.6.7",
    "keen-slider": "^6.8.5",
    "nprogress": "^0.2.0",
    "pretty-cache-header": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-wrap-balancer": "^0.4.0",
    "remix-i18next": "^5.1.0",
    "remix-utils": "^6.0.0",
    "swiper": "^11.0.5",
    "tailwind-merge": "^1.10.0",
    "tailwindcss": "^3.2.7",
    "tailwindcss-animate": "^1.0.5",
    "valibot": "^0.29.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@remix-run/dev": "^1.19.3",
    "@remix-run/eslint-config": "^1.19.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "tailwind-scrollbar": "^3.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14"
  },
  "volta": {
    "node": "16.7.0"
  },
  "lint-staged": {
    "app/**/{*.ts,*.tsx}": "npm run lint"
  }
